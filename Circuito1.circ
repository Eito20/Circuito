<project version="1.0">
  <lib desc="Gates" name="0"/>
  <main name="main">
    <circuit name="Paridade4Bits">
      <comp lib="0" loc="(100,100)" name="Pin de Entrada" name="A"/>
      <comp lib="0" loc="(100,140)" name="Pin de Entrada" name="B"/>
      <comp lib="0" loc="(100,180)" name="Pin de Entrada" name="C"/>
      <comp lib="0" loc="(100,220)" name="Pin de Entrada" name="D"/>
      
      <comp lib="0" loc="(200,120)" name="XOR Gate"/>
      <comp lib="0" loc="(300,160)" name="XOR Gate"/>
      <comp lib="0" loc="(400,200)" name="XOR Gate"/>
      
      <comp lib="0" loc="(500,240)" name="Pin de Saída" name="S"/>
      
      <!-- Conexões -->
      <wire from="(100,100)" to="(200,120)"/>
      <wire from="(100,140)" to="(200,120)"/>
      <wire from="(200,120)" to="(300,160)"/>
      <wire from="(100,180)" to="(300,160)"/>
      <wire from="(300,160)" to="(400,200)"/>
      <wire from="(100,220)" to="(400,200)"/>
      <wire from="(400,200)" to="(500,240)"/>
    </circuit>
  </main>
</project>
